<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="550.0" prefWidth="760.0" spacing="20.0" styleClass="body" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.virtualbank.controllers.newAccountController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label prefHeight="103.0" prefWidth="222.0" styleClass="hero-customer" text="New account">
        <VBox.margin>
            <Insets bottom="-27.0" top="-54.0" />
        </VBox.margin>
      <padding>
         <Insets bottom="25.0" />
      </padding></Label>

    <Label styleClass="hero-customer-description" text="Select the type for your new account">
        <VBox.margin>
            <Insets bottom="20.0" />
        </VBox.margin>
      <padding>
         <Insets bottom="29.0" top="-33.0" />
      </padding></Label>

    <Button contentDisplay="CENTER" onAction="#onRegularButtonClick" prefHeight="91.0" prefWidth="153.0" styleClass="menu-btn" text="Regular" textAlignment="CENTER" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets bottom="-20.0" top="10.0" />
        </VBox.margin></Button>

    <Button contentDisplay="CENTER" onAction="#onSavingsButtonClick" prefHeight="91.0" prefWidth="153.0" styleClass="menu-btn" text="Savings Account" textAlignment="CENTER" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets left="-400.0" top="-89.0" />
        </VBox.margin></Button>

    <Button contentDisplay="CENTER" onAction="#onSpecialButtonClick" prefHeight="91.0" prefWidth="153.0" styleClass="menu-btn" text="Special" textAlignment="CENTER" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets bottom="-20.0" right="-400.0" top="-114.0" />
        </VBox.margin></Button>

    <Button contentDisplay="CENTER" onAction="#onBackButtonClick" prefHeight="41.0" prefWidth="153.0" styleClass="logout-btn" text="Back to menu" textAlignment="CENTER" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets bottom="-112.0" left="5.0" right="5.0" top="41.0" />
        </VBox.margin></Button>

    <stylesheets>
        <URL value="@styles/menuStyles.css" />
    </stylesheets>

</VBox>
