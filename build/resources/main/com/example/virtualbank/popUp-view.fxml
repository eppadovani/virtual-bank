<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="183.0" prefWidth="272.0" spacing="10.0" styleClass="body" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

    <Label fx:id="popUpMsg" styleClass="hero-description">
        <VBox.margin>
            <Insets bottom="20.0" />
        </VBox.margin></Label>

    <Button fx:id="okBtn" contentDisplay="CENTER" prefHeight="41.0" prefWidth="128.0" styleClass="login-btn" text="Ok" textAlignment="CENTER" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets top="10.0" />
        </VBox.margin></Button>

    <stylesheets>
        <URL value="@styles/styles.css" />
    </stylesheets>

</VBox>
